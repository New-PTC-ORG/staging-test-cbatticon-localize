#: cbatticon.c:163
msgid "Display the version"
msgstr "Mostra la versione"

#: cbatticon.c:164
msgid "Display debug information"
msgstr "Mostra informazioni di debug"

#: cbatticon.c:165
msgid "Set update interval (in seconds)"
msgstr "Imposta intervallo di aggiornamento (in secondi)"

#: cbatticon.c:166
msgid "Set icon type ('standard', 'notification' or 'symbolic')"
msgstr "Imposta tipo di icona (“standard”, “notifica” o “simbolica”)"

#: cbatticon.c:167
msgid "Set low battery level (in percent)"
msgstr "Imposta livello batteria scarica (in percentuale)"

#: cbatticon.c:168
msgid "Set critical battery level (in percent)"
msgstr "Imposta livello batteria critico (in percentuale)"

#: cbatticon.c:169
msgid "Command to execute when low battery level is reached"
msgstr "Comando da eseguire quando si raggiunge il livello di batteria scarica"

#: cbatticon.c:170
msgid "Command to execute when critical battery level is reached"
msgstr "Comando da eseguire quando si raggiunge il livello di batteria critico"

#: cbatticon.c:171
msgid "Command to execute when left clicking on tray icon"
msgstr "Comando da eseguire quando si fa clic con il tasto sinistro sull'icona nella barra"

#: cbatticon.c:173
msgid "Hide the notification popups"
msgstr "Nascondi i popup di notifica"

#: cbatticon.c:175
msgid "List available icon types"
msgstr "Elenca i tipi di icona disponibili"

#: cbatticon.c:176
msgid "List available power supplies (battery and AC)"
msgstr "Elenca le fonti di alimentazione disponibili (batteria e corrente alternata)"

#: cbatticon.c:180
msgid "[BATTERY ID]"
msgstr "[ID BATTERIA]"

#: cbatticon.c:184
#, c-format
msgid "Cannot parse command line arguments: %s\n"
msgstr "Impossibile analizzare gli argomenti della riga di comando: %s\n"

#: cbatticon.c:195
msgid ""
"cbatticon: a lightweight and fast battery icon that sits in your system "
"tray\n"
msgstr "cbatticon: un'icona della batteria leggera e veloce che si posiziona nella barra di sistema\n"

#: cbatticon.c:196
#, c-format
msgid "version %s\n"
msgstr "versione %s\n"

#: cbatticon.c:204
msgid "List of available power supplies:\n"
msgstr "Elenco delle fonti di alimentazione disponibili:\n"

#: cbatticon.c:219
msgid "List of available icon types:\n"
msgstr "Elenco dei tipi di icona disponibili:\n"

#: cbatticon.c:220 cbatticon.c:221 cbatticon.c:222
msgid "available"
msgstr "disponibile"

#: cbatticon.c:220 cbatticon.c:221 cbatticon.c:222
msgid "unavailable"
msgstr "non disponibile"

#: cbatticon.c:236
#, c-format
msgid "Unknown icon type: %s\n"
msgstr "Tipo di icona sconosciuto: %s\n"

#: cbatticon.c:248
msgid "No icon type found!\n"
msgstr "Nessun tipo di icona trovato!\n"

#: cbatticon.c:255
#, c-format
msgid "Invalid update interval! It has been reset to default (%d seconds)\n"
msgstr "Intervallo di aggiornamento non valido! È stato ripristinato al valore predefinito (%d secondi)\n"

#: cbatticon.c:262
#, c-format
msgid "Invalid low level! It has been reset to default (%d percent)\n"
msgstr "Livello di batteria scarica non valido! È stato ripristinato al valore predefinito (%d percento)\n"

#: cbatticon.c:267
#, c-format
msgid "Invalid critical level! It has been reset to default (%d percent)\n"
msgstr "Livello critico non valido! È stato ripristinato al valore predefinito (%d percento)\n"

#: cbatticon.c:273
msgid ""
"Critical level is higher than low level! They have been reset to default\n"
msgstr "Il livello critico è più alto del livello di batteria scarica! Sono stati ripristinati ai valori predefiniti\n"

#: cbatticon.c:367 cbatticon.c:402
#, c-format
msgid "type: %-*.*s\tid: %-*.*s\tpath: %s\n"
msgstr "tipo: %%-*.*s\tid: %%-*.*s\tpercorso: %s\n"

#: cbatticon.c:367
msgid "Battery"
msgstr "Batteria"

#: cbatticon.c:402
msgid "AC"
msgstr "CA"

#: cbatticon.c:424
#, c-format
msgid "Cannot open sysfs directory: %s (%s)\n"
msgstr "Impossibile aprire la directory sysfs: %s (%s)\n"

#: cbatticon.c:431
#, c-format
msgid "No battery with suffix %s found!\n"
msgstr "Nessuna batteria con suffisso %s trovata!\n"

#: cbatticon.c:436
msgid "No battery nor AC power supply found!\n"
msgstr "Nessuna batteria né alimentazione CA trovata!\n"

#: cbatticon.c:784 cbatticon.c:786
msgid "AC only, no battery!"
msgstr "Solo CA, nessuna batteria!"

#: cbatticon.c:910
#, c-format
msgid "Spawning low battery level command in 5 seconds: %s"
msgstr "Avvio del comando per livello batteria scarica tra 5 secondi: %s"

#: cbatticon.c:915
msgid "Skipping low battery level command, no longer discharging"
msgstr "Comando per livello batteria scarica saltato, non più in scarica"

#: cbatticon.c:921 cbatticon.c:923
#, c-format
msgid "Cannot spawn low battery level command: %s\n"
msgstr "Impossibile avviare il comando per livello batteria scarica: %s\n"

#: cbatticon.c:928
msgid "Cannot spawn low battery level command!"
msgstr "Impossibile avviare il comando per livello batteria scarica!"

#: cbatticon.c:938
#, c-format
msgid "Spawning critical battery level command in 30 seconds: %s"
msgstr "Avvio del comando per livello batteria critico tra 30 secondi: %s"

#: cbatticon.c:943
msgid "Skipping critical battery level command, no longer discharging"
msgstr "Comando per livello batteria critico saltato, non più in scarica"

#: cbatticon.c:949 cbatticon.c:951
#, c-format
msgid "Cannot spawn critical battery level command: %s\n"
msgstr "Impossibile avviare il comando per livello batteria critico: %s\n"

#: cbatticon.c:956
msgid "Cannot spawn critical battery level command!"
msgstr "Impossibile avviare il comando per livello batteria critico!"

#: cbatticon.c:971 cbatticon.c:973
#, c-format
msgid "Cannot spawn left click command: %s\n"
msgstr "Impossibile avviare il comando per clic sinistro: %s\n"

#: cbatticon.c:978
msgid "Cannot spawn left click command!"
msgstr "Impossibile avviare il comando per clic sinistro!"

#: cbatticon.c:1042
msgid "Battery is missing!"
msgstr "La batteria è mancante!"

#: cbatticon.c:1046
msgid "Battery status is unknown!"
msgstr "Lo stato della batteria è sconosciuto!"

#: cbatticon.c:1050
msgid "Battery is charged!"
msgstr "La batteria è carica!"

#: cbatticon.c:1054
#, c-format
msgid "Battery is discharging (%i%% remaining)"
msgstr "La batteria si sta scaricando (%i%% rimanenti)"

#: cbatticon.c:1058
#, c-format
msgid "Battery is not charging (%i%% remaining)"
msgstr "La batteria non si sta caricando (%i%% rimanenti)"

#: cbatticon.c:1062
#, c-format
msgid "Battery level is low! (%i%% remaining)"
msgstr "Il livello della batteria è basso! (%i%% rimanenti)"

#: cbatticon.c:1066
#, c-format
msgid "Battery level is critical! (%i%% remaining)"
msgstr "Il livello della batteria è critico! (%i%% rimanenti)"

#: cbatticon.c:1070
#, c-format
msgid "Battery is charging (%i%%)"
msgstr "La batteria si sta caricando (%i%%)"

#: cbatticon.c:1099
#, c-format
msgid "%d minute"
msgid_plural "%d minutes"
msgstr[0] "%d minuto"
msgstr[1] "%d minuti"

#: cbatticon.c:1100
#, c-format
msgid "%d hour, %s remaining"
msgid_plural "%d hours, %s remaining"
msgstr[0] "%d ora, %s rimanenti"
msgstr[1] "%d ore, %s rimanenti"

#: cbatticon.c:1102
#, c-format
msgid "%d minute remaining"
msgid_plural "%d minutes remaining"
msgstr[0] "%d minuto rimanente"
msgstr[1] "%d minuti rimanenti"
